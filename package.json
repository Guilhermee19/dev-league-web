{
  "name": "boilerplate-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build && node compress.js",
    "build:prod": "ng build --configuration production && node compress.js",
    "compress": "node compress.js",
    "prepare": "husky",
    "commit": "git-cz",
    "lint": "ng lint",
    "clear": "rm -rf node_modules && rm package-lock.json && rm -rf .angular && npm install",
    "s3": "aws s3 cp dist s3://bucket-do-s3 --recursive",
    "s3:prod": "aws s3 cp dist s3://bucket-do-s3 --recursive",
    "build:s3": "ng build && npm run s3",
    "build:prod:s3": "ng build --configuration production && npm run s3:prod",
    "cloudfront": "aws cloudfront create-invalidation --distribution-id ID_AQUI --paths '/*'",
    "post-release": "ts-node ./version.ts && git c \"chore(env): update enviroment versions\" && git push --tags",
    "release:ma": "standard-version --release-as major && npm run post-release",
    "release:mi": "standard-version --release-as minor && npm run post-release",
    "release:p": "standard-version --release-as patch && npm run post-release",
    "serve:ssr:boilerplate-web": "node dist/boilerplate-web/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/service-worker": "^17.3.0",
    "@angular/ssr": "^17.3.10",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "express": "^4.18.2",
    "md5-typescript": "^1.0.5",
    "medium-zoom": "^1.1.0",
    "moment": "^2.30.1",
    "ngx-cookie-service": "^17.1.0",
    "primeflex": "^3.3.1",
    "primeng": "^17.18.11",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
